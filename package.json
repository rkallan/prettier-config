{
    "name": "rkallan-prettier-config",
    "version": "1.0.0",
    "description": "Shareable Prettier config by R. Kallan",
    "keywords": [
        "Prettier",
        "Prettier config"
    ],
    "bugs": {
        "url": "https://github.com/rkallan/prettier-config/issues/new",
        "email": "r.kallan@upcmail.nl"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:rkallan/prettier-config.git"
    },
    "license": "MIT",
    "author": "Ravi Kallan <r.kallan@upcmail.nl>",
    "main": "index.json",
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "scripts": {
        "commit": "git-cz"
    },
    "dependencies": {
        "@prettier/plugin-xml": "~1.0.2"
    },
    "devDependencies": {
        "cz-conventional-changelog": "^3.3.0",
        "semantic-release": "~21.0.1"
    },
    "peerDependencies": {
        "prettier": ">=2.4.1"
    },
    "plugins": [
        "@semantic-release/commit-analyzer",
        "@semantic-release/release-notes-generator",
        "@semantic-release/changelog",
        "@semantic-release/github",
        "@semantic-release/npm",
        "@semantic-release/git"
    ]
}
